        <TextView
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/graphikmedium"
            android:text="Password Requirements"
            android:textColor="#1B1A23"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout20" />

        <ImageView
            android:id="@+id/lowercase_image"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:background="@drawable/oval_gray"
            app:layout_constraintBottom_toBottomOf="@+id/textView39"
            app:layout_constraintStart_toStartOf="@+id/textView38"
            app:layout_constraintTop_toTopOf="@+id/textView39" />

        <TextView
            android:id="@+id/textView39"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/graphikregular"
            android:text="Must have at least 1 lowercase letter"

            app:layout_constraintStart_toEndOf="@+id/lowercase_image"
            app:layout_constraintTop_toBottomOf="@+id/textView38" />

        <ImageView
            android:id="@+id/uppercase_image"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:background="@drawable/oval_gray"
            app:layout_constraintBottom_toBottomOf="@+id/textView40"
            app:layout_constraintStart_toStartOf="@+id/textView38"
            app:layout_constraintTop_toTopOf="@+id/textView40" />

        <TextView
            android:id="@+id/textView40"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/graphikregular"
            android:text="Must have at least 1 uppercase letter"

            app:layout_constraintStart_toEndOf="@+id/uppercase_image"
            app:layout_constraintTop_toBottomOf="@+id/textView39" />

        <ImageView
            android:id="@+id/at_number_image"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:background="@drawable/oval_gray"
            app:layout_constraintBottom_toBottomOf="@+id/textView41"
            app:layout_constraintStart_toStartOf="@+id/textView38"
            app:layout_constraintTop_toTopOf="@+id/textView41" />

        <TextView
            android:id="@+id/textView41"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/graphikregular"
            android:text="Must have at least 1 number"

            app:layout_constraintStart_toEndOf="@+id/at_number_image"
            app:layout_constraintTop_toBottomOf="@+id/textView40" />

        <ImageView
            android:id="@+id/six_len_image"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:background="@drawable/oval_gray"
            app:layout_constraintBottom_toBottomOf="@+id/textView42"
            app:layout_constraintStart_toStartOf="@+id/textView38"
            app:layout_constraintTop_toTopOf="@+id/textView42" />

        <TextView
            android:id="@+id/textView42"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/graphikregular"
            android:text="Must have 6 characters minimum"

            app:layout_constraintStart_toEndOf="@+id/six_len_image"
            app:layout_constraintTop_toBottomOf="@+id/textView41" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/next_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="30dp"
            android:background="@drawable/light_blue_gradient_button"
            android:backgroundTint="@color/in_active_btn_color"
            android:fontFamily="@font/graphikregular"
            android:text="Next â€” Business information"
            android:textAllCaps="false"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
